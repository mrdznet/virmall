.infoAmount .term .wst-co-between{width: 460rpx; height: 86rpx; float: right; margin-right: 10rpx; color: #777; font-size: 29rpx; text-align: right; text-overflow: ellipsis; white-space: nowrap; overflow: hidden;}
.wst-co-window{width:100%; height:500rpx; position:fixed; bottom:-120rpx; left:0; z-index:9999999; background:white;}
.wst-co-window .co-title{position: relative; height:80rpx; line-height:80rpx;font-size: 32rpx; text-align:center; border-bottom: 1px solid #f1f1f1;}
.wst-co-window .co-title .icon{position: absolute; top:12rpx; right:12rpx; width: 60rpx; height:60rpx;}
.wst-co-window .co-content{width:auto; height:360rpx; padding: 0rpx 0 40rpx 0;background:#f6f6f8;font-size: 26rpx}
.wst-co-window .co-content .option{padding:0 20rpx;background:white;}
.wst-co-window .co-content .option label{display:block;height:80rpx; line-height:80rpx;border-bottom: 1px solid #EDEDED;}
.wst-co-window .co-content .option radio{float: right;}
radio .wx-radio-input{width:20px;height:20px;}
.gc_item{width:45%;height: 100%;float: left;margin-right:10rpx;}
.gc_item_left,.gc_item_main,.gc_item_right{float: left}
.gc_item_left i{display: block;font-style: normal;width: 0;height: 1px;border: 2px solid rgba(0,0,0,0);border-right: 3px solid #d82a2e;margin-left: -1px;}
.gc_item_right i{display: block;font-style: normal;width: 0;height: 1px;border: 2px solid rgba(0,0,0,0);border-left: 3px solid #d82a2e;margin-left: -1px;}
.gc_item_main{width:91%;height: 100%;background-color: #d82a2e;color: #fff;font-size: 24rpx;overflow: hidden;display: flex;justify-content: center;align-items: center;}
.wst-co-window{height:600rpx  !important;z-index: 99999;}
.wst-co-content{background-color: #f6f6f8;height:500rpx !important;margin-top:0  !important;padding:0rpx 10rpx 20rpx 10rpx  !important;}
.wst-co-list{ border: 0 ;}
.wst-co-list .list{margin-top: 20rpx; height:200rpx; padding: 0 20rpx;}
.wst-co-list .list .left{float: left; width: 35%; height: 100%;}
.coupon_left{height: 100%;float:left;width: 10rpx;background:url(data:image/png;base64,) 0 0; background-size: 130%;}
.coupon_left i{display:block;font-style:normal;width:0;height:1px;border:4rpx solid rgba(0,0,0,0);border-right: 6rpx solid #ee3c3c;margin-left: -2rpx;}
.coupon_left_main{position: relative;height: 89%;background: #ee3c3c;float: left;width: 91%;padding:5px 5px 5px 0px;}
.coupon_left_main_inner{width: 100%;height: 100%;border:2rpx solid #fff;border-left-style: dashed;text-align: center;}
.coupon_left_txt1{display: block;margin-top: 20rpx; color: #fff;font-size: 66rpx;font-weight: bold;}
.coupon_left_txt1 .yan{font-weight: normal;font-size: 26rpx;}
.coupon_left_txt2{display: block; font-size: 30rpx;color:#fff;}
.wst-co-list .list .right{float: left; width: 65%; height: 100%; background: #fff; position: relative;}
.wst-co-list .list .right p{display: block; padding: 0 20rpx; max-width: 100%; overflow: hidden; white-space: nowrap; text-overflow: ellipsis;}
.c_item_txt1{font-size: 30rpx;margin-top:20rpx;font-weight:bold;color:#333;}
.c_item_txt2{font-size: 28rpx;color:#555;}
.c_item_txt3{font-size: 28rpx;color:#000;margin: 6rpx 0;}
.c_item_txt4{font-size: 26rpx;color:#999;}
.c_item_button{font-size: 26rpx;color: #d82a2e;background: #fff;border: 1rpx solid #d82a2e;border-radius: 30rpx;display: block;position: absolute;top: 37%;height: 56rpx;line-height:56rpx;padding: 0 12rpx;right: 6rpx;}