.animation{width: 100%; height: 600rpx; position: fixed; bottom: 0rpx; left: 0rpx; background: white; z-index: 1001;}
.animation .wst-top-title{width: 100%; height: 80rpx; position: relative; border-bottom: 3rpx solid #b1b1b1;}
.animation .wst-top-title .title{width: 100%; height: 80%; margin-top: 15rpx; margin-bottom: 10rpx; position: absolute; z-index: -999; text-align: center;}
.animation .wst-top-title icon{float: right; margin: 10rpx 10rpx 0rpx 10rpx;}
.animation .navigation{width: 100%; height: 42rpx; margin-top: 15rpx; border-bottom: 3rpx solid #b1b1b1;}
.animation .list{margin-left: 20rpx;}
.animation .navigation .main{width: 150rpx; height: 40rpx; margin-left: 20rpx; text-align: center; float: left; font-size: 33rpx; text-overflow: -o-ellipsis-lastline; overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 1; -webkit-box-orient: vertical; border-bottom: 3rpx solid red;}
.animation .navigation .too{margin-left: 0rpx;}
.animation .navigation .tooed{border-bottom: 3rpx solid transparent;}
.animation .list{width: 100%; height: 400rpx; font-size: 33rpx;}
.animation .list .main{width: 100%; height: 60rpx; line-height: 65rpx;}
