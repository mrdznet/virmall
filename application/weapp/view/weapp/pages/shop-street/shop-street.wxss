.screen-tier{width: 90vw;height:100vh;overflow-y: hidden; position: fixed;top: 0rpx;right:0rpx;background: white ;z-index: 9999;font-size: 32rpx;color: #666;}
.screen-tier .screen-mol{height: auto;width: 90vw;}
.screen-tier .screen-mol .title{height: 80rpx;line-height: 80rpx;padding-left: 10rpx;}
