.option-box1 input{padding: 10rpx;margin: 6rpx; text-align: center;background-color: #f0f2f5;border-radius: 10rpx;}
.option-box1 .rail{height: 40rpx;border-bottom: 1rpx solid #666;width: 30rpx;margin:0rpx 10rpx 0rpx 10rpx;}
