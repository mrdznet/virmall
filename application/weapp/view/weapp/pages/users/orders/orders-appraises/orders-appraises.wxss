page{
  width: 100%;
  height: 100%;
  background: #f6f6f8;
}
.container{
  width: 100%;
  height: auto;
}
.wst-or-ap{
  margin-top: 10rpx;
  background: #fff;
}
.wst-or-ap .shop{
  padding:0 20rpx;
  font-size:30rpx;
  height:60rpx;
  line-height:60rpx;
  border-bottom:  1rpx solid #e9e8e8;
}
.wst-or-ap .goods{
  padding: 10rpx 20rpx;
  border-bottom: 1rpx solid #e9e8e8;
}
.wst-or-ap .goods .img{
  float:left;
  width: 130rpx;
  height: 130rpx;
}
.wst-or-ap .goods .img image{
  width: 130rpx;
  height: 130rpx;
}
.wst-or-ap .goods .names{
  float:left;
  width: 420rpx;
  padding: 10rpx 10rpx 0 10rpx;
}
.wst-or-ap .goods .name{
  width: 420rpx;
  height: 80rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.wst-or-ap .goods .spec{
  width: 420rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 26rpx;
  color: #ccc;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.wst-or-ap .goods .click{
  float:left;
  width: 140rpx;
  font-size: 28rpx;
  padding-top: 32rpx;
  text-align: right;
}
.wst-or-ap .goods .click .button{
  color:#e00102;
  font-size: 26rpx;
  border: 1rpx solid #e00102;
}
.wst-or-ap .evaluate{
  border-top: 10rpx solid #f6f6f8;
}
.wst-or-ap .evaluate .list{
  font-size: 28rpx;
  padding: 16rpx 20rpx;
}
.wst-or-ap .evaluate .list .left{
  float:left;
  width: 20%;
}
.wst-or-ap .evaluate .list .right{
  float:left;
  width: 80%;
  position: relative;
}
.wst-or-ap .evaluate .list .right text{
  position: absolute;
  top:0;
  right:60rpx;
  color:#d85600;
}
.wst-stars{
  float:left;
  width: 40rpx;
  height: 40rpx;
  margin-right:10rpx;
}
.wst-stars.star0{
  background:url('data:image/png;base64,') 0rpx 0rpx no-repeat;
  background-size: 100%;
}
.wst-stars.star1{
  background:url('data:image/png;base64,') 0rpx 0rpx no-repeat;
  background-size: 100%;
}
.wst-or-ap .content{
  margin-top:20rpx;
  padding: 0 20rpx;
  font-size: 28rpx;
}
.wst-or-ap .content textarea{
  width: 99%;
  height:200rpx;
  padding: 20rpx 0;
  font-size: 28rpx;
  border: 1rpx solid #a6a5a6;
}
.wst-or-ap .imgs{
  height:120rpx;
  padding: 20rpx 20rpx 0 20rpx;
}
.wst-or-ap .imgs .img{
  float:left;
  width: 120rpx;
  height:120rpx;
  margin-right:20rpx;
  position: relative;
}
.wst-or-ap .imgs .img image{
  width: 120rpx;
  height:120rpx;
}
.wst-or-ap .imgs .delete{
  position: absolute;
  top:0;
  right:0;
  width:40rpx;
  height:40rpx;
  background: url('data:image/png;base64,')  0rpx 0rpx no-repeat;
  background-size:100%
}
.wst-or-ap .upload{
  padding: 20rpx 20rpx 0 20rpx;
}
.wst-or-ap .submit{
  margin-top:20rpx;
  padding: 20rpx 20rpx;
  border-bottom: 10rpx solid #f6f6f8;
}
.wst-or-ap .submit .button{
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  background-color: #e00102;
  color: white;
  border-radius: 6rpx;
  font-size:30rpx;
}
.wst-or-ap .submit .button:active,.wst-or-ap .submit .button[disabled][type="default"],.wst-or-ap .submit .button[disabled]:not([type]){
  color:#fff0f0;
  background:#f52f30;
  background-clip:padding-box;
}