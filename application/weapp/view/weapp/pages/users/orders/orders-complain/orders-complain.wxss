page{
  width: 100%;
  height: 100%;
  overflow-y: hidden;
  background: #F6F6F8;
}
.container{
  width: 100%;
  height:auto;
  overflow: hidden;
}
.wst-bor-all{
  width: 100%;
  height: auto;
  font-size: 32rpx;
  overflow: hidden;
}
.wst-bor-all .mar-t{
  margin-top: 15rpx;
}
.wst-bor-all .val{
  color:red;
  margin-top: 15rpx;
  display: inline-block;
}
.wst-bor-all .arr{
  width: 15rpx;
  height: 15rpx;
  margin-top: 25rpx;
  margin-left: 10rpx;
  border-right: 5rpx solid #333333;
  border-bottom: 5rpx solid #333333;
  transform: rotate(-45deg); 
}
.wst-bor-all .exart{
  display: flex;
}
.wst-bor-all .strip{
  width: 100%;
  height: 68rpx;
  display: flex;
  justify-content: space-between;
  background: white;
  line-height: 68rpx;
  border-bottom: 2rpx solid #EAEAEA;
}
.wst-bor-all .strip .title{
  margin-left: 15rpx;
}
.wst-bor-all .strip .status{
  margin-right: 15rpx;
  margin-top: 0rpx;
}
.wst-bor-all .strip .time-val{
  margin-right: 15rpx;
}
.wst-bor-all .comm-detail{
  width: 100%;
  height: 100%;
  display: flex;
  background: white;
  justify-content: space-between;
}
.wst-bor-all .comm-detail .image{
  width: 120rpx;
  height: 120rpx;
  margin: 10rpx 30rpx 10rpx;
}
image{
  width:100%;
  height:100%;
}
.wst-bor-all .comm-detail .name{
  width: 400rpx;
  height: 120rpx;
  margin-top: 10rpx;
  font-size: 25rpx;
}
.wst-bor-all .comm-detail .info{
  width: 130rpx;
  height: 130rpx;
  text-align: top;
  font-size: 30rpx;
}
.wst-bor-all .order-val{
  width: 100%;
  height: 170rpx;
  display: flex;
  justify-content: space-between;
  text-align: right;
  background: white;
}
.wst-bor-all .order-val .title{
  margin-left: 15rpx;
  margin-top: 10rpx;
} 
.wst-bor-all .order-val .value{
  margin-right: 15rpx;
}
.wst-bor-all .cmpn-content{
  width: 100%;
  height: auto;
  overflow: hidden;
  background: white;
}
.wst-bor-all .cmpn-content .content{
  width: 99%;
  height: 240rpx;
  border: 2rpx solid #A9A9A9;
}
.wst-bor-all .cmpn-content .content textarea{
  width: 100%;
  height: 100%;
}
::-webkit-scrollbar{
  width: 0rpx;
  height: 0rpx;
}
.wst-bor-all .imgs{
  height:120rpx;
  padding: 20rpx 20rpx 0 20rpx;
}
.wst-bor-all .imgs .img{
  float:left;
  width: 120rpx;
  height:120rpx;
  margin-right:20rpx;
  position: relative;
}
.wst-bor-all .imgs .img image{
  width: 120rpx;
  height:120rpx;
}
.wst-bor-all .imgs .delete{
  position: absolute;
  top:0;
  right:0;
  width:40rpx;
  height:40rpx;
  background:url('data:image/png;base64,') 0 0 no-repeat;
  background-size:100%;
}

.wst-bor-all .upload{
  padding: 20rpx 20rpx 0 20rpx;
}
 .submit{
  margin-top:20rpx;
  padding: 20rpx 20rpx;
  border-bottom: 10rpx solid #f6f6f8;
}
 .submit .button{
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  background-color: #e00102;
  color: white;
  border-radius: 6rpx;
  font-size:30rpx;
}
.wst-bor-all .submit .button:active,.wst-bor-all .submit .button[disabled][type="default"],.wst-bor-all .submit .button[disabled]:not([type]){
  color:#fff0f0;
  background:#f52f30;
  background-clip:padding-box;
}
.drawer_screen{
  width: 100%;  
  height: 100%;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index: 1000;  
  background: #000;  
  opacity: 0.4;  
  overflow: hidden; 
  border-top: 1rpx solid white; 
}
.model-1{
  width: 100%;
  height: 600rpx;
  position: absolute;
  bottom: 0;
  z-index: 1001;
  background: #fff;
  border: 1px solid transparent;
  overflow: hidden;
}
.model-1 .top{
   height: 80rpx;
   display: flex;
   align-items: center;
  justify-content: space-between;
}
.model-1 .content{
  border: 1px solid transparent;
  width: 100%;
  height: 420rpx;  
}
.model-1 .content .list2{
  display: flex;
   height: 80rpx;
   align-items: center;
   margin:0 20rpx 0 20rpx;
  justify-content: space-between;
  border-bottom: 2rpx solid #EAEAEA;
}
.model-1 .content .list2 .title{
  font-size: 30rpx;
  line-height: 50rpx;
}