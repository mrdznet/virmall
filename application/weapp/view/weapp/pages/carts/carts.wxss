@import "../../addons/closure/reward/reward.wxss";
page{
  width: 100%;
  background: #f6f6f8;
}
.container{
  width: 100%;
  border-bottom: 96rpx solid transparent;
}
.remind-model{
  position: relative;
  top: 200rpx;
  margin: 0 auto;
}
.remind-model .image{
  width: 200rpx;
  height: 200rpx;
  margin:0 auto;
}
.remind-model .image .img{
  width: 100%;
  height: 200rpx;
}
.remind-model .content{
  width: 100%;
  margin-top: 50rpx;
  font-size: 32rpx;
  text-align: center;
}
.remind-model .button{
  padding-top:30rpx;
  margin:0 60rpx;
}
.remind-model .button button{
  width: 300rpx;
  height:70rpx;
  display:flex;
  justify-content:center;
  align-items:center;
  text-align:center;
  background-color:#fff;
  color:#df0202;
  font-size:30rpx;
  border: 2rpx solid #df0202;
}
.remind-model .button button:active,.remind-model .button button[disabled][type="default"],.remind-model .button button[disabled]:not([type]){
  background: #f2f2f2;
  background-clip:padding-box;
}
/*列表渲染*/
.list-model{
  height: auto;
  margin-top: 20rpx;
  padding: 30rpx 20rpx 0 20rpx;
  background: white;
  overflow: hidden;
  position: relative;
}
.list-model .store-info{
  height: 60rpx; 
  line-height:60rpx;
  background: white;
  font-size:30rpx;
  padding-bottom: 3px;
  border-bottom: 1rpx solid #edebeb;
}
.list-model icon{
  float: left;
  margin-top: 10rpx;
  margin-left: 10rpx;
  height:56rpx;
}
.list-model .store-name{
  float: left;
  margin-left: 20rpx;
}
.list-model .store-name i{
  float: left;
  width: 44rpx;
  height: 60rpx;
  position: relative;
  background:url('data:image/png;base64,') 0 14rpx no-repeat;
  background-size: 87%;
}
.list-model .commodify{
  padding: 10rpx 0;
  border-bottom: 1rpx solid #F1F1F1;
  overflow: hidden;
}
.list-model .commodify icon{
  height:56rpx;
  margin-top: 50rpx;
}
.list-model .commodify .img{
  width:130rpx;
  height:130rpx;
  float: left;
  margin-left: 20rpx;
}
.list-model .commodify .img image{
  width:130rpx;
  height:130rpx;
}
/*信息内容*/
.list-model .details{
  float: right;
  width: 500rpx;
  padding-left:10rpx;
  padding-bottom:10rpx;
  display: flex;
  font-size: 30rpx;
}
.list-model .title{
  width: 350rpx;
  line-height:40rpx;
   font-size: 28rpx;
  font-weight: 700;
}
.list-model .price{
  width: 150rpx;
  height: 46rpx;
  line-height:46rpx;
  text-align: right;
  font-size: 26rpx;
}
.list-model .spec{
  float: right;
  width: 500rpx;
  padding-left:10rpx;
  padding-bottom:10rpx;
  color: #a6a6a6;
  font-size: 28rpx;
}
/*数量条*/
.list-model .number-change{
  float: right;
  width: 300rpx;
  height: 60rpx;
  display: flex;
  position: relative;
}
.list-model .subtract{
  width: 55rpx;
  height: 60rpx;
  line-height:60rpx;
  text-align: center;
  border: 1rpx solid #ddd;
  border-top-left-radius: 6rpx;
  border-bottom-left-radius: 6rpx;
}
.list-model .number{
  width: 80rpx;
  height: 60rpx;
  line-height:60rpx;
  text-align: center;
  border-top: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
  color: #e00102;
  font-size: 30rpx;
}
.list-model .add{
  width: 55rpx;
  height: 60rpx;
  line-height:60rpx;
  text-align: center;
  border: 1rpx solid #ddd;
  border-top-right-radius: 6rpx;
  border-bottom-right-radius: 6rpx;
}
.list-model .prompt{
  position: absolute;
  top:-40rpx;
  left:45rpx;
  font-size: 26rpx;
  color: #e00102;
}
.list-model .number.back{
  background: #facccc;
}
/*垃圾桶*/
.list-model .icon{
  float: right;
  width: 50rpx;
  height: 50rpx;
  margin-left:60rpx;
}
.list-model .icon image{
  width: 100%;
  height: 100%;
  float:right;
}
/*统计*/
.list-model .total{
  height: 50rpx;
  padding:20rpx 0 25rpx 0;
  display: flex;
  font-size: 30rpx;
}
.list-model .total .left{
  width: 50%;
  height: 42rpx;
  line-height:42rpx;
}
.list-model .total .right{
  width: 50%;
  height: 42rpx;
  line-height:42rpx;
  color: #de0202;
  text-align: right;
}
/*底部导航条*/
.wst-ca-bottom{
  width: 100%;
  height: 84rpx;
  border-top: 1rpx solid #e8e8e8;
  position: fixed;
  z-index: 999;
  left: 0rpx;
  bottom: -2rpx;
  background: white;
}
.wst-ca-bottom .bottom{
  height: 84rpx;
  line-height:84rpx;
  padding: 0 20rpx;
}
.wst-ca-bottom .icon{
  float: left;
  height: 56rpx; 
  margin-top: 6rpx;
  margin-left: 10rpx;
}
.wst-ca-bottom .whole{
  float: left;
  margin-left: 12rpx;
  font-size: 33rpx;
  color: #2c2c2c;
}
.wst-ca-bottom .total{
  float: right;
  color: #2c2c2c;
  font-size: 33rpx;
  margin-right: 20rpx;
}
.wst-ca-bottom .total text{
  color: #de0202;
}
.wst-ca-bottom .settlement{
  float: right;
  height: 70rpx;
  margin-top: 8rpx;
}
.wst-ca-bottom .settlement button{
  width: 160rpx;
  background-color: #e00102;
  color: white;
  border-radius: 6rpx;
  font-size:30rpx;
}
.wst-ca-bottom .settlement button:active,.wst-ca-bottom .settlement button[disabled][type="default"],.wst-ca-bottom .settlement button[disabled]:not([type]){
  color:#fff0f0;
  background:#f52f30;
  background-clip:padding-box;
}