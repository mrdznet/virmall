/*授权*/
.wst-or-frame{
  background: #fff;
  width: 80%;
  -webkit-background-clip: padding-box;
  background-clip: padding-box;
  pointer-events: auto;
  position: relative;
  font-size: 30rpx;
  border-radius: 4rpx;
  color:#202020;
}
.wst-or-frame .title{
  color:#000000;
  font-size: 32rpx;
  text-align: center;
  padding:20rpx 0;
  border-bottom: 2rpx solid #f2f1f1;
}
.wst-or-frame .title2{
  color:#000000;
  font-size: 28rpx;
  padding:50rpx 40rpx;
}
.wst-or-frame .title2 i{
  float: left;
  width: 40rpx;
  height:40rpx;
  margin-right: 10rpx;
  background:url('data:image/png;base64,') 0 0 no-repeat;
  background-size: 100%;
}
.wst-or-frame .button{
  height:80rpx;
  border-top: 2rpx solid #f2f1f1;
}
.wst-or-frame .button .cancel{
  width: 50%;
  height:80rpx;
  color:#000000;
  background: #fff;
  font-size: 32rpx;
  border-right: 1rpx solid #f2f1f1;
}
.wst-or-frame .button .authorize{
  width: 50%;
  height:80rpx;
  color:#1AAD19;
  background:#fff;
  font-size: 32rpx;
}
.wst-or-frame .button button::after{
  content:" ";
  width:200%;
  height:200%;
  position:absolute;
  top:0;
  left:0;
  border:1rpx solid #fff;
  -webkit-transform:scale(0.5);
  transform:scale(0.5);
  -webkit-transform-origin:0 0;
  transform-origin:0 0;
  box-sizing:border-box;
  border-radius: 0rpx;
}
.wst-or-frame .button button:active{
  background:#efefef;
  border-radius: 0rpx;
}
