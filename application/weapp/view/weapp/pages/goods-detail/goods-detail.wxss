@import "../../addons/closure/reward/reward.wxss";
@import "../../addons/closure/coupon/coupon.wxss";
page{width: 100%; height: 100%; background: #F6F6F8;}
.container{width: 100%; height: 100%; border-bottom: 90rpx solid transparent;}
/*顶部导航条*/
.navigetor-top{width: 100%; height: 80rpx; display: flex; justify-content: flex-end; position: fixed; left: 0; top: 0; text-align: center; font-size: 32rpx; background: white; z-index: 999;}
.navigetor-top .left{width: 20%; height: 100%; border-bottom: 4rpx solid transparent; line-height: 75rpx;}
.navigetor-top .lefted{width: 20%; height: 77rpx;color: #de0202;border-bottom: 4rpx solid #de0202; line-height: 75rpx;}
.navigetor-top .between{width: 20%; height: 100%; border-bottom: 4rpx solid transparent; line-height: 75rpx;}
.navigetor-top .betweened{width: 20%; height: 77rpx;color: #de0202;border-bottom: 4rpx solid #de0202; line-height: 75rpx;}
.navigetor-top .right{width: 20%; height: 100%; border-bottom: 4rpx solid transparent; line-height: 75rpx;}
.navigetor-top .righted{width: 20%; height: 77rpx;color: #de0202;border-bottom: 4rpx solid #de0202; line-height: 75rpx;}
.navigetor-top .commocart{width: 10%; height: 100%; margin-left:46rpx; margin-right:10rpx; position: relative;}
.navigetor-top .commocart .img{width: 56%; height: 56%; margin: 15rpx 5rpx 10rpx;}
.navigetor-top .commocart .num{position: absolute; top:0; right:0; min-width: 30rpx; height: 30rpx; color: #fff; font-size:26rpx; border-radius: 40rpx; background: #e8121e;}
/*轮播图*/
.slider-model,.slider-model swiper,.slider-model swiper image{width: calc(100%); width: -webkit-calc(100%); width: -moz-calc(100%); width: -ms-calc(100%); width: -o-calc(100%); height: calc(100vw); height: -webkit-calc(100vw); height: -moz-calc(100vw); height: -ms-calc(100vw); height: -o-calc(100vw); position: relative;}
.wst-go-mores{position: absolute;top:20rpx;right:20rpx;z-index:10;width: 60rpx;height: 60rpx;background-color: rgba(0,0,0,.4);border-radius: 100%;}
.wst-go-mores i{position: absolute;top: 6rpx;left: 6rpx;width: 48rpx;height: 48rpx;line-height: 48rpx;font-size: 48rpx;color:#fff;text-align: center;z-index: 0;}
.wst-go-more{position: absolute;top: 58rpx;right: 10rpx;z-Index: 10000;width: 32%;}
.wst-go-more .arrow{position: absolute;top:-12rpx;right:14rpx;display: block;width: 0;height: 0;line-height: 0;border-width:22rpx;border-color: transparent transparent #fff transparent;border-style: dashed dashed solid dashed;}
.wst-go-more .more{margin-top:26rpx;border-radius:3px;background:#fff;display: block;overflow: hidden;}
.wst-go-more .term{float: left;box-sizing: border-box;width: 100%;}
.wst-go-more .term:active{background-color: #e5e6e7;padding-left: 6rpx;margin-left: 0;}
.wst-go-more .term:active{background-image: none;border-top-color: #e5e6e7;}
.wst-go-more .more .column{width: 160rpx;display: block;margin: 0 auto;height: 46rpx;line-height: 46rpx;padding:20rpx 0px;}
.wst-go-more .more .line{border-bottom: 2rpx solid #f2f2f2;}
.wst-go-more .more i{float: left;width: 46rpx;height: 46rpx;margin-left:4rpx;position: relative;}
.wst-go-more .home{background:url('data:image/png;base64,') 0 0 no-repeat; background-size: 100%;}
.wst-go-more .category{background:url('data:image/png;base64,') 0 0 no-repeat; background-size: 100%;}
.wst-go-more .cart{background:url('data:image/png;base64,') 0 0 no-repeat; background-size: 100%;}
.wst-go-more .follow{background:url('data:image/png;base64,') 0 0 no-repeat; background-size: 100%;}
.wst-go-more .user{background:url('data:image/png;base64,') 0 0 no-repeat; background-size: 100%;}
.wst-go-more .more p{float: left;width: 110rpx;font-size:26rpx}
.wst-go-more .more li:active{padding-left:4rpx;}
/*标题*/
.commodity-title{padding: 20rpx 20rpx 0 20rpx; font-size: 32rpx; color: #051B28; background: #fff; line-height: 46rpx;}
/*价格信息*/
.commodity-info{padding: 20rpx 20rpx 10rpx 20rpx;background: #fff;}
.commodity-info .price{font-size:46rpx;color: #de0202;}
.commodity-info .price i{font-size: 26rpx;}
.commodity-info .price .market {margin-left: 24rpx;font-size: 26rpx;color: #999;text-decoration: line-through;}
.commodity-info .info{padding-top: 10rpx;font-size: 26rpx;color: #999;display: -webkit-box;width: 100%;}
.commodity-info .info .term{float: none;-webkit-box-flex: 1;width: 0;}
/*促销模块 1 */
.promotion_one{width: 100%; height: 90rpx; border: 1rpx solid #F6F6F8; background: white; font-size: 30rpx; display: flex;}
.promotion_one .grade{width: 80%; height: 60%; margin: 15rpx 15rpx 10rpx; border: 1rpx solid transparent; display: flex; line-height: 55rpx;}
.promotion_one .icon{width: 100rpx; height: 70%; border: 1rpx solid #DF3033; text-align: center; font-weight: bold; color: #DF3033; line-height: 40rpx; margin-top: 5rpx; margin-right: 10rpx;}
.promotion_one .grade .strip{margin: 0rpx 10rpx 0rpx; line-height: 45rpx; color: #DFDEDE;}
.promotion_one .radius{font-size: 40rpx; font-weight: bolder; color: #DFDEDE; margin-left: 20rpx;}
/*促销模块 2 */
.promotion_two{width: 100%; height: 120rpx; border: 1rpx solid #F6F6F8; background: white; font-size: 30rpx; display: flex; position: relative;}
.promotion_two .grade{width: 80%; height: 40%; margin: 15rpx 15rpx 10rpx; border: 1rpx solid transparent; display: flex; line-height: 55rpx;}
.promotion_two .explain{width: 300rpx; height: 40rpx; font-size: 25rpx; position: absolute; left: 130rpx; top: 70rpx; box-sizing: border-box; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 1; -webkit-box-orient: vertical; overflow: hidden;}
.promotion_two .icon{width: 100rpx; height: 80%; border: 1rpx solid #DF3033; text-align: center; font-weight: bold; color: #DF3033; line-height: 40rpx; margin-top: 5rpx; margin-right: 10rpx;}
.promotion_two .grade .strip{margin: 0rpx 10rpx 0rpx; line-height: 45rpx; color: #DFDEDE;}
.promotion_two .radius{font-size: 40rpx; font-weight: bolder; color: #DFDEDE; margin-left: 20rpx;}
/*评论模块*/
.wst-term{margin:20rpx 0;}
.comment{height: 90rpx;padding:0 20rpx;background: white; font-size: 28rpx; display: flex;position: relative;border-bottom: 1rpx solid #edebeb;}
.comment:active {background-color: #f2f2f2;}
.comment .title{display: flex; width: 95%; border: 1rpx solid transparent;line-height: 90rpx;}
.comment .title .strip{margin: 0rpx 10rpx 0rpx; line-height: 80rpx; color: #DFDEDE;}
.comment .number{color: #de0202;}
.comment .radius{width: 15%;font-size: 40rpx; font-weight: bold; color: #dbdada;line-height: 70rpx;text-align: right;}
/*店铺详情*/
.storedetail{padding:20rpx;background: #fff;}
.storedetail .image{width: 120rpx; height: 120rpx;float: left;}
.storedetail .image image{width: 120rpx; height: 120rpx; float: left;}
.storedetail .info{width: 80%; height: 100rpx; float: left; overflow: hidden;}
.storedetail .name{ float: left;width: 100%;padding-left:10rpx;font-size: 33rpx;line-height: 70rpx;}
.storedetail .variety{float: left;width: 100%;padding-left:10rpx;font-size:26rpx;color: #848689;}
/*评分详情*/
.graded{padding: 16rpx 20rpx;font-size: 26rpx;background: #fff;padding-bottom: 20rpx;border-bottom: 2rpx solid #f2f1f1;}
.graded .content{display: flex; padding-top: 15rpx; padding-bottom: 15rpx; box-sizing: border-box; font-size: 30rpx;}
.graded .between{width:33%;}
.graded .strip{color: #edebeb;}
.graded .size{color: #de0202;}
/*购买咨询*/
.message{ height: 76rpx;line-height: 76rpx; display: flex; background: white;position: relative;}
.message image{width: 50rpx; height: 50rpx;margin-top: 14rpx;margin-left: 20rpx;}
.message .content{width: auto; height: auto; margin-left: 20rpx; font-size: 30rpx;}
.message .icon{width: 50rpx; height: 50rpx; background:url('data:image/png;base64,') 0 0 no-repeat;background-size: 105%;position: absolute;top: 16rpx;right: 25rpx;}
.recommend{width: 100%; height: auto; border: 1rpx solid transparent;}
.wst-consult{padding:20rpx;background:#fff;border-top: 2rpx solid #f2f1f1;}
.wst-consult .content{padding:10rpx 0 10rpx 50rpx;font-size: 26rpx;color: #848689;line-height: 36rpx;position: relative;}
.wst-consult .content image{position: absolute;left:0;top:5rpx;width: 40rpx; height: 40rpx;}
/*分隔栏*/
.background{background: #F6F6F8;}
.background .image{width: 50%; height: 70rpx; border: 1rpx solid transparent; margin-left: 170rpx; position: relative;}
.background image{width: 100%; height: 50%; margin-top: 15rpx;}
.background .content{position: absolute; top: 10rpx; left: 120rpx; font-size: 30rpx;}
/*商品模块*/
.recommend .commodity{width: 30%; height: 350rpx; border: 1rpx solid transparent; float: left; margin: 10rpx 10rpx 10rpx; background: white;}
.recommend .commodity .image{width: 200rpx; height: 200rpx; border: 1rpx solid transparent; margin: 10rpx 10rpx 10rpx;}
.recommend .commodity .image image{width: 100%; height: 100%;}
.recommend .commodity .title{width: 200rpx; height: 75rpx; line-height: 36rpx; border: 1rpx solid transparent; margin-left: 10rpx; box-sizing: border-box; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; overflow: hidden; font-size:26rpx;}
.recommend .commodity .price{width: 200rpx; height: 40rpx; margin: 10rpx 10rpx 10rpx; border: 1rpx solid transparent; font-size: 30rpx; color: #E00102;}
/*底部导航条*/
.navigation-bottom{width: 100%; height: 84rpx; position: fixed; bottom: 0;left:0;display: -webkit-box;background: white;border-top: 1rpx solid #e8e8e8;}
.navigation-bottom .left-model{box-sizing: border-box;-webkit-box-flex: 3;float: none;width: 0;}
.navigation-bottom .left-model .list{width: 100%;display: -webkit-box;}
.navigation-bottom .left-model .term{float: none;-webkit-box-flex: 1;width: 0;}
.navigation-bottom .left-model .image{width:46rpx;height:38rpx;margin: 0 auto;margin-top: 10rpx;margin-bottom: 2rpx;}
.navigation-bottom .left-model image{width:46rpx;height:38rpx;}
.navigation-bottom .left-model .content{font-size: 26rpx; height: 40rpx; line-height:40rpx;text-align:center;}
.navigation-bottom .right-model{box-sizing: border-box;-webkit-box-flex: 4;float: none;width: 0;}
.navigation-bottom .right-model .left{float: left;width: 50%;height: 84rpx;line-height: 84rpx;font-size: 30rpx;color: #fff;background: #f9a517;border-radius:0;}
.navigation-bottom .right-model .right{float: left;width: 50%;height: 84rpx;line-height: 84rpx;font-size: 30rpx;color: #fff;background: #e00102;border-radius:0;}
.right-model .left:active,.right-model .left[disabled][type="default"],.right-model .left[disabled]:not([type]){color:#fbf6ee; background:#f3c77c;background-clip:padding-box;}
.right-model .right:active,.right-model .right[disabled][type="default"],.right-model .right[disabled]:not([type]){color:#fbd6d6; background:#f25a5b;background-clip:padding-box;}
.navigation-bottom .right-model .whole{width: 100%; height: 88%; float: right; background-color: #E00102; color: white; font-size: 30rpx; margin-top: 6rpx; margin-right: 10rpx; border-radius: 6rpx;}
.right-model .whole:active,.right-model .whole[disabled][type="default"],.right-model .whole[disabled]:not([type]){color:#fbd6d6; background:#f25a5b; background-clip:padding-box;}
.navigation-bottom .right-model button::after{-webkit-transform:scale(0);transform:scale(0);}
/*详情版面*/
.detailsStatus{width: 100%;height:auto;padding: 10rpx;background-color: #fff;padding-top: 100rpx;}
.detailsStatus image{width: 100%;height:auto;}
/*评论*/
.commentStatus{width: 100%; height: auto; border: 1rpx solid transparent;}
.commentStatus .remind{width: 100%; height: 500rpx; margin-top: 300rpx; text-align: center; font-size: 34rpx;}
::-webkit-scrollbar{width: 0rpx; height: 0rpx;}
.wst-go-evaluate{width: 100%; height: auto; position: relative; margin-top: 30rpx; background: white;}
.wst-go-evaluate .top{width: 94%; height: 50rpx; border: 1rpx solid transparent; position: relative; margin: 0rpx 20rpx 0rpx; padding:15rpx 0rpx;}
.wst-go-evaluate .top  .headPhoto{float: left; width: 50rpx;height: 50rpx;border-radius: 50%;margin-right: 5rpx;}
.wst-go-evaluate .userName{width: auto; height: 50rpx; border: 1rpx solid transparent; float: left; font-size: 35rpx; }
.wst-go-evaluate .userGrade{width: 60rpx; height: 32rpx; border: 1rpx solid transparent; float: left;}
.wst-go-evaluate .userGrade image{width: 32rpx; height: 32rpx; float: left;}
.wst-go-evaluate .commentGrade{width: 32rpx; height: 32rpx; border: 1rpx solid transparent; float: left;}
.wst-go-evaluate .commentGrade image{width: 32rpx; height: 32rpx; float: left;}
.wst-go-evaluate .commentTime{width: auto; height: 50rpx; border: 1rpx solid transparent; float: right; color: #8e8d8d; font-size: 30rpx; margin-top: -5rpx;}
.wst-go-evaluate .bottom{border: 1rpx solid transparent; margin: 0rpx 20rpx 15rpx; padding:5rpx 0; font-size: 35rpx;}
.wst-go-evaluate .imageslist{padding:5rpx 0;}
.wst-go-evaluate .images{float:left; margin-right:6rpx; width: 120rpx; height: 120rpx;}
.wst-go-evaluate .images .img{width: 120rpx; height: 120rpx;}
.wst-go-evaluate .spec{margin-top:6rpx; font-size: 30rpx; color: #8e8d8d;}
.model1{width: 100%; height: 500rpx; background: black; position: fixed; bottom: 0rpx;}
/*content*/
.drawer_attr_box{width: 100%; height: 500rpx; position: fixed; bottom: 0; left: 0; z-index: 9999; background: white;}
.drawer_attr_box2{height: 800rpx;}
.drawer_attr_box .model-top{width: 100%; height: 180rpx; border-bottom: 1rpx solid #DFDEDE; position: relative; display: flex;}
.drawer_attr_box .model-top .image{width: 160rpx; height: 160rpx; padding: 10rpx 10rpx 0 10rpx; position: relative; top: -30rpx; margin-left: 20rpx; background: white;border:2rpx solid #f2f1f1;}
.drawer_attr_box .model-top image{width: 160rpx; height: 160rpx;}
.drawer_attr_box .model-top .content{width: 400rpx; height: 140rpx; border: 1rpx solid transparent; float: left; margin: 10rpx 20rpx 0;}
.drawer_attr_box .model-top .content .title{border: 1rpx solid transparent; height: 85rpx; font-size: 28rpx; padding: 0rpx 0rpx; box-sizing: border-box; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; overflow: hidden;}
.drawer_attr_box .model-top .price{width: auto; height: 50rpx; border: 1rpx solid transparent; float: left; font-size: 32rpx; line-height: 50rpx; color: #DF3033;}
.drawer_attr_box .model-top .old-price{color: #707070; font-size: 26rpx; text-decoration: line-through; margin-left: 20rpx;}
.drawer_attr_box .model-top .icon_out{width: 54rpx; height: 54rpx; border: 1rpx solid transparent; position: absolute; right: 5rpx; top: 10rpx;}
.drawer_attr_box .model-top .icon_out image{width: 54rpx; height: 54rpx;}
/*动态弹窗-底部*/
.drawer_attr_box .model-bottom{position: absolute; bottom:90rpx; left:0; width: 100%; height: 180rpx; overflow: hidden; font-size: 30rpx;}
.drawer_attr_box .model-bottom .border{width: 93%; height: auto; border: 1rpx solid transparent; margin: 45rpx 20rpx 0;}
.drawer_attr_box .model-bottom .content{width: 100%; height: 40rpx; font-size: 28rpx;}
.drawer_attr_box .model-bottom .surplus{position: relative; width: 100%; height: 70rpx; border: 1rpx solid transparent; display: flex;}
.drawer_attr_box .model-bottom .left{margin-top: 20rpx; display: flex;}
.drawer_attr_box .model-bottom .number1{width: auto; height: 100%; border: 1rpx solid transparent;}
.drawer_attr_box .model-bottom .right{position: absolute; right:20rpx; top:0rpx; border: 1rpx solid transparent; height: 100%; width: 200rpx; display: flex; text-align: center; line-height: 60rpx;}
.drawer_attr_box .model-bottom .right .icon_subtract{width: 40px; height: 64rpx; border: 1rpx solid #DFDEDE;}
.drawer_attr_box .model-bottom .right .number{width: 40px; height: 64rpx; border-top: 1rpx solid #DFDEDE;border-bottom: 1rpx solid #DFDEDE;    color: #de0202;}
.drawer_attr_box .model-bottom .right .icon_add{width: 40px; height: 64rpx; border: 1rpx solid #DFDEDE;}
.wst-go-spec{width:auto; height: 350rpx; padding:10rpx;}
.wst-go-spec .term{padding: 20rpx 0; border-bottom: 1px solid #f1f1f1;}
.wst-go-spec .spec{font-size:26rpx;}
.wst-go-spec .img{float: left; width: 90rpx; height: 90rpx; border-radius: 4rpx; margin: 10rpx 12rpx 0 0;}
.wst-go-spec .img.active{width: 86rpx; height: 86rpx; border: 1px solid #e00102;}
.wst-go-spec .option{float: left; font-size:26rpx; margin: 10rpx 12rpx 0 0; padding: 8rpx 12rpx; border: 1px solid #a0a0a0; border-radius: 6rpx; text-align: center;}
.wst-go-spec .option.active{color: #fff; background: #e00102; border: 1px solid #e00102;}
/*按钮*/
.drawer_attr_box .button{position: absolute; bottom:20rpx; left:0; width: 100%; height: 70rpx; overflow: hidden; margin: 0rpx auto;}
.drawer_attr_box .button button{width: 80%; line-height: 70rpx; font-size:30rpx; background: #E00102; color: white;}
.drawer_attr_box .button button:active,.drawer_attr_box .button button[disabled][type="default"],.drawer_attr_box .button button[disabled]:not([type]){color:#fff0f0; background:#f52f30; background-clip:padding-box;}
/*参数*/
.wst-go-window{width:100%; height:500rpx; position:fixed; bottom:0; left:0; z-index:9999; background:white;}
.wst-go-window .title{position: relative; height:80rpx; line-height:80rpx; text-align:center; border-bottom: 1px solid #f1f1f1;}
.wst-go-window .title .icon{position: absolute; top:12rpx; right:12rpx; width: 60rpx; height:60rpx;}
.wst-go-window .content{width:auto; height:360rpx; margin-top:20rpx; padding: 0rpx 20rpx 40rpx 20rpx;}
.wst-go-window .attrs{font-size:30rpx;}
/*评论可视层*/
.digital-layer{border-top: 80rpx solid transparent;width: 100vw;height: 9vh;background-color: #fff;display: flex;flex-direction: row;flex-wrap:nowrap; }
.digital-layer .model{width: 20vw;text-align:center;padding:14rpx 0rpx;}